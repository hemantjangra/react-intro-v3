{
  "name": "react-intro",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:client": "parcel build --public-url ./dist/ src/index.html",
    "build:server": "parcel build -d dist-server --target node server/index.js",
    "build": "yarn run build:client && yarn run build:server",
    "start": "yarn -s run build && node dist-server/index.js",
    "dev": "parcel src/index.html --port 2345",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" "
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/preset-react": "7.12.13",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.1",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/postcss7-compat": "2.0.3",
    "autoprefixer": "9.8.6",
    "eslint": "7.18.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "parcel": "1.12.3",
    "parcel-bundler": "^1.12.3",
    "postcss": "7.0.35",
    "prettier": "^2.3.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@2.0.3"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@loadable/server": "^5.15.1",
    "express": "^4.17.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
